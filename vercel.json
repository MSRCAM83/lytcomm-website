{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "CI=false npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "fluid": true,
  "functions": {
    "api/pdf-import.js": {
      "maxDuration": 300,
      "memory": 1024
    }
  },
  "crons": [
    { "path": "/api/cron-digest", "schedule": "0 13 * * *" }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/" }
  ]
}
